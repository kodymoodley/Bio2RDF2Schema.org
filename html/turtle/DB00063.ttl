	<http://schema.org/name> "Eptifibatide" ;
	<http://schema.org/description> "Synthetic cyclic hexapeptide that binds to platelet receptor glycoprotein and inhibits platelet aggregation." ;
	<http://schema.org/url> "https://www.drugbank.ca/drugs/DB00063" ;
	<http://schema.org/doseSchedule> "2 mg/mL Injection, solution form with intravenous route" ;
	<http://schema.org/legalStatus> "investigational" ;
