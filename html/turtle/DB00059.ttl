
<http://bio2rdf.org/drugbank:DB00059> a <http://schema.org/Drug> ;
	<http://schema.org/name> "Pegaspargase" ;
	<http://schema.org/description> "Pegylated L-asparagine amidohydrolase from E. coli. Pegylation substantially (by a factor of 4) extends the protein half life." ;
	<http://schema.org/url> "https://www.drugbank.ca/drugs/DB00059" ;
	<http://schema.org/doseSchedule> "750 [iU]/mL Injection, solution form with intramuscular; intravenous route" ;
	<http://schema.org/legalStatus> "investigational" ;
	<http://schema.org/mechanismOfAction> "Pegaspargase, more effective than asparaginase, converts asparagine to aspartic acid and ammonia. It facilitates production of oxaloacetate which is needed for general cellular metabolism. Some malignant cells lose the ability to produce asparagine and so the loss of exogenous sources of asparagine leads to cell death." ;
	<http://schema.org/alternateName> "Putative L-asparaginase precursor" ;
	<http://schema.org/sameAs> "https://www.drugbank.ca/drugs/DB00059" .
