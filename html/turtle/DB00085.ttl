
<http://bio2rdf.org/drugbank:DB00085> a <http://schema.org/Drug> ;
	<http://schema.org/name> "Pancrelipase" ;
	<http://schema.org/description> "Pancrelipase is an enzyme mixture isolated from porcine or bovine pancreas, sometimes called pancreatin. It contains 3 enzymes: amylase, lipase, and a protease (chymotrypsin). Pancrelipase is marketed under several brand names such as Ultresa™ and Viokace™." ;
	<http://schema.org/url> "https://www.drugbank.ca/drugs/DB00085" ;
	<http://schema.org/doseSchedule> "17500; 4200; 10000 [USP'U]/1; [USP'U]/1; [USP'U] Capsule, delayed release form with oral route" ;
	<http://schema.org/legalStatus> "approved" ;
	<http://schema.org/mechanismOfAction> "The lipase, protease and amylase components of pancrelipase break down fat, protein, and starches, respectively, in the small intestine. Lipase hydrolyzes fats into glycerol and fatty acids. Protease converts proteins into proteoses and derived substances, while amylase converts starches into dextrins and sugars." ;
	<http://schema.org/alternateName> "PA" ;
	<http://schema.org/sameAs> "https://www.drugbank.ca/drugs/DB00085" .
